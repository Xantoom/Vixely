# EditorConfig helps developers define and maintain consistent
# coding styles between different editors and IDEs
# editorconfig.org

root = true

[*]
charset = utf-8
insert_final_newline = true
end_of_line = lf
indent_style = tab
indent_size = 4

[**/*.{yml,yaml}]
indent_style = space

[**.md]
indent_size = unset
indent_style = unset
