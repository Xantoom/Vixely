{
	"name": "vixely",
	"private": true,
	"type": "module",
	"scripts": {
		"postinstall": "./scripts/copy-jassub.sh",
		"dev": "vite",
		"build:wasm": "./scripts/build-wasm.sh",
		"build:routes": "node ./scripts/generate-route-tree.mjs",
		"generate:routes": "bun run build:routes",
		"build:prep": "bun run build:wasm && bun run build:routes",
		"build:web": "vite build",
		"build": "bun run build:prep && bun run build:web",
		"lint": "oxlint src --type-aware --type-check",
		"lint:fix": "oxlint src --type-aware --type-check --fix",
		"fmt": "oxfmt src '!src/routeTree.gen.ts' --write",
		"fmt:check": "oxfmt src '!src/routeTree.gen.ts' --check"
	},
	"dependencies": {
		"@mediabunny/ac3": "^1.34.4",
		"@tanstack/react-query": "^5.90.21",
		"@tanstack/react-router": "^1.161.1",
		"jassub": "^2.4.1",
		"lucide-react": "^0.563.0",
		"mediabunny": "^1.34.4",
		"react": "^19.2.4",
		"react-dom": "^19.2.4",
		"react-helmet-async": "^2.0.5",
		"sonner": "^2.0.7",
		"zustand": "^5.0.11"
	},
	"devDependencies": {
		"@tailwindcss/vite": "^4.2.0",
		"@tanstack/react-router-devtools": "^1.161.1",
		"@tanstack/router-plugin": "^1.161.1",
		"@types/bun": "latest",
		"@types/react": "^19.2.14",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.4",
		"oxfmt": "^0.28.0",
		"oxlint": "^1.48.0",
		"oxlint-tsgolint": "^0.13.0",
		"tailwindcss": "^4.2.0",
		"vite": "8.0.0-beta.13",
		"vite-plugin-wasm": "^3.5.0"
	},
	"peerDependencies": {
		"typescript": "^5.9.3"
	}
}
